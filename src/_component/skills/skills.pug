-
    var header = '#skills';
    var whatCanIDoList = [
        {
            title: 'Front End',
            skillList: [
                'HTML5',
                'CSS3',
                'JavaScript',
                'SASS',
                'PUG / Jade',
                'Gulp',
                'PhotoShop'
            ]
        },
        {
            title: 'Back End',
            skillList: [
                'Git',
                'JSON',
                'NodeJs'
            ]
        },
        {
            title: 'Other',
            skillList: [
                'Windows',
                'Linux',
                'Android',
                'iOS',
                'Word',
                'Excel',
                'PowerPoint'
            ]
        }
    ];

#what-can-i-do
.what-can-i-do
    .what-can-i-do__header=header
    each whatCanIDo in whatCanIDoList
        .what-can-i-do__title=whatCanIDo.title
        .what-can-i-do__list
            each skill in whatCanIDo.skillList
                .what-can-i-do__skill=skill